{
  "name": "@variousjs/create",
  "version": "2.0.1",
  "description": "VariousJS boilerplate",
  "bin": "bin/create.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "tsc --noemit && eslint . --ext .ts,.tsx,.js",
    "start": "NODE_ENV=development webpack --progress --config ./webpack/component.js & webpack serve --config ./webpack/entry.js --progress & npm run css:dev",
    "build": "webpack --progress --config ./webpack/base.js && npm run css:build",
    "css:build": "npx tailwindcss -i ./src/shadcn-ui/globals.css -o ./public/globals.css --minify",
    "css:dev": "npx tailwindcss -i ./src/shadcn-ui/globals.css -o ./public/globals.css --watch",
    "prepublishOnly": "node bin/copy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/variousjs/boilerplate.git"
  },
  "keywords": [
    "template",
    "variousjs"
  ],
  "author": "LoeiFy <LoeiFy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/variousjs/boilerplate/issues"
  },
  "homepage": "https://github.com/variousjs/boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.19.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@variousjs/registry": "^0.1.5",
    "@variousjs/various": "^3.1.0",
    "babel-loader": "^8.2.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "core-js": "^3.25.1",
    "css-loader": "^6.3.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "style-loader": "^3.3.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.2",
    "webpack": "^5.78.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.7.3"
  }
}
